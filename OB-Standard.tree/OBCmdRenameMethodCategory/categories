
!OBCmdRenameMethodCategory methodsFor: 'accessing' stamp: 'lr 9/22/2010 21:39'!
categories
	| categories |
	categories := Set new.
	target theClass allSuperclasses
		do: [ :each | categories addAll: each organization categories ].
	PackageOrganizer default packages
		do: [ :each | categories add: each methodCategoryPrefix ].
	target theClass organization categories
		do: [ :each | categories remove: each ifAbsent: [ ] ].
	categories
		remove: ClassOrganizer nullCategory ifAbsent: [ ];
		remove: ClassOrganizer default ifAbsent: [ ].
	^ categories! !
