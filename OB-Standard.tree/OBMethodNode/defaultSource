
!OBMethodNode methodsFor: 'private' stamp: 'lr 2/12/2009 10:41'!
defaultSource
	^ String streamContents: 
		[ :stream | 
		self writeMethodHeaderTo: stream.
		stream
			nextPut: Character cr;
			tab;
			nextPutAll: '"This method does not exist."';
			nextPut: Character cr;
			tab;
			nextPutAll: 'self halt.' ]! !
