
!OBCmdRenameCategory methodsFor: 'execution' stamp: 'lr 9/24/2010 21:57'!
execute
	| categories category |
	categories := self categories sorted.
	category := OBCompletionRequest new
		assisted: true;
		default: target name;
		collection: categories;
		prompt: 'Rename Category';
		signal.
	category isNil ifTrue: [ ^ self ].
	target renameTo: category.
	self 
		select: (target setName: category)
		with: requestor announcer! !
