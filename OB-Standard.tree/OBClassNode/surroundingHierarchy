
!OBClassNode methodsFor: 'accessing' stamp: 'dr 10/23/2008 13:28'!
surroundingHierarchy
	| class result |
	result := OrderedCollection new.
	class := self theNonMetaClass.
	class allSuperclasses reverseDo: 
		[:aClass | 
		result add: aClass].

	class allSubclassesWithLevelDo:
		[:aClass :level |
		result add: aClass]
	 	startingLevel: 0.	

	^ result! !
