
!OBDefinitionPanel methodsFor: '*ob-standard-cmds' stamp: 'lr 12/10/2010 08:53'!
referencesToIt: aClassName 
	| class |
	class := self environment
		at: aClassName
		ifAbsent: [ nil ].
	class isBehavior 
		ifFalse: [ ^ self sendersOfIt: aClassName ].
	OBReferencesBrowser browseRoot: (OBClassNode on: class).
	^ true! !
