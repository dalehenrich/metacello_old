
!OBButtonBar methodsFor: 'updating' stamp: 'lr 4/7/2010 08:00'!
updateCommands
	| cmds |
	cmds := model commands.
	cmds size = self submorphs size 
		ifTrue: [
			cmds with: self submorphs 
				do:[ :cmd :button| self updateButton: button for: cmd ] ]
		ifFalse: [
			self removeAllMorphs.
			cmds do: [ :ea | self addButtonFor: ea ] ]! !
