
!OBMorphicPlatform methodsFor: 'interaction' stamp: 'lr 7/19/2010 13:00'!
handleConfirmationRequest: request
	| choice |
	choice := UIManager default
		confirm: request prompt
		trueChoice: request okChoice
		falseChoice: request cancelChoice.
	^ choice
		ifNil: [ false ]
		ifNotNil: [ choice ]! !
