
!OBSystemWindow methodsFor: 'events' stamp: 'lr 12/3/2010 15:20'!
keyStroke: anEvent
	super keyStroke: anEvent.
	(anEvent shiftPressed and: [ anEvent commandKeyPressed and: [ anEvent keyCharacter = $T ] ]) 
		ifTrue: [ self selectCommand. ^ true ].
	(anEvent shiftPressed and: [ anEvent commandKeyPressed and: [ anEvent keyCharacter = $W ] ]) 
		ifTrue: [ self selectWindow. ^ true ].
	^ false! !
