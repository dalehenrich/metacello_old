
!MetacelloVersionQueryTestCase methodsFor: 'tests' stamp: 'dkh 10/21/2009 14:05'!
testBaseline10PackagesForSpecNamed05
	| project version expected list |
	project := self exampleProject.
	version := project version: '1.0-baseline'.
	expected := #('Example-Core' 'Example-AddOn' 'Example-Tests' ).
	list := version packagesForSpecNamed: 'No UI'.
	self assert: (list size = expected size).
	list do: [:pkg |
		self assert: (expected includes: pkg name) ].! !
