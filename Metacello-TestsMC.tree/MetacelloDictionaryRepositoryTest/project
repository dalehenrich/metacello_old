
!MetacelloDictionaryRepositoryTest methodsFor: 'accessing' stamp: 'DaleHenrichs 9/22/2010 15:37'!
project

	| constructor project |
	"Construct Metacello project"
	constructor := MetacelloVersionConstructor on: self.
	project := constructor project.
	project loader: 
		((project loaderClass new)
			shouldDisablePackageCache: true;
			yourself).
	project loadType: self loadType.
	^project! !
