
!MetacelloStackCacheTestCase methodsFor: 'tests' stamp: 'DaleHenrichs 1/31/2010 15:16'!
testClearCache

	MetacelloPlatform current 
		stackCacheFor: #cached
		at: #key 
		doing: [:cache | | value |
			cache at: #x put: 1.
			self assert: (cache at: #x ifAbsent: []) == 1.
			MetacelloClearStackCacheNotification signal.
			self assert: (cache at: #x ifAbsent: []) == nil ]
! !
