
!OBColumn methodsFor: 'callbacks' stamp: 'lr 4/25/2011 10:56'!
keystroke: aCharacter from: aMorph
	| scan nodes |
	scan := self announcer announce: OBNodeCommandScan.
	nodes := panel columns 
		collect: [ :each | each parent ].
	nodes := (nodes copyWith: self selectedNode)
		select: [ :each | each notNil ].
	nodes reverseDo: [ :each |
		(scan processKeystroke: aCharacter asKeystroke withNode: each for: self)
			ifTrue: [ ^ self ] ]! !
