
!OBCommandCluster methodsFor: 'accessing' stamp: 'lr 4/8/2010 11:07'!
groupedCommands
	| groups |
	groups := commands 
				groupBy: [:svc | svc group]
				having: [:group | group isEmpty not].
	groups := groups collect: [:ea | ea asArray sort: [:a :b | a order <= b order]].
	^ groups asArray sort: [:a :b | a anyOne group <= b anyOne group]! !
