
!OBKeystroke methodsFor: 'printing' stamp: 'lr 5/20/2011 19:21'!
printOn: aStream
	self isShift
		ifTrue: [ aStream nextPutAll: 'shift+' ].
	self isControl
		ifTrue: [ aStream nextPutAll: 'ctrl+' ].
	self isOption
		ifTrue: [ aStream nextPutAll: 'opt+' ].
	self isCommand
		ifTrue: [ aStream nextPutAll: 'cmd+' ].
	aStream nextPut: self key! !
