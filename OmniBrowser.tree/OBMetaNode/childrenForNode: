
!OBMetaNode methodsFor: 'nodes' stamp: 'dr 3/5/2008 14:11'!
childrenForNode: aNode
	| chosenEdges | 
	chosenEdges := self edgesForNode: aNode.
	^ chosenEdges gather: 
		[:edge | 
		filters 
			inject: (edge nodesForParent: aNode)
			into: [:list :filter | filter nodesFrom: list forNode: aNode]].! !
